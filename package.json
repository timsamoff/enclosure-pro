{
  "name": "enclosure-pro",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/timsamoff/enclosure-pro"
  },
  "description": "Professional effects pedal enclosure drill hole designer",
  "main": "electron/main.js",
  "license": "GPL-3.0",
  "author": "Tim Samoff <samoff@gmail.com>",
  "maintainers": [
    "Tim Samoff <samoff@gmail.com>"
  ],
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "clean": "rimraf dist release build",
    "clean:release": "rimraf release",
    "precopy-icons": "node -e \"const fs = require('fs'); if (!fs.existsSync('build')) fs.mkdirSync('build', { recursive: true }); console.log('Build directory ready')\"",
    "copy-icons": "node scripts/copy-icons.js",
    "electron:build": "npm run build && npm run copy-icons && npm run electron:build:only",
    "electron:build:only": "electron-builder --publish=never",
    "electron:build:clean": "npm run clean:release && npm run electron:build",
    "electron:build:full": "npm run clean && npm run electron:build",
    "electron:build:installer": "npm run build && npm run copy-icons && electron-builder --win nsis --publish=never",
    "electron:build:portable": "npm run build && npm run copy-icons && electron-builder --win portable --publish=never",
    "check": "tsc"
  },
  "build": {
    "appId": "com.enclosurepro.app",
    "productName": "Enclosure Pro",
    "publish": {
      "provider": "github",
      "owner": "timsamoff",
      "repo": "enclosure-pro"
    },
    "artifactName": "Enclosure-Pro-${version}.${ext}",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "images/**/*",
      "build/**/*",
      "scripts/**/*"
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "fileAssociations": [
      {
        "ext": "enc",
        "name": "Enclosure Project File",
        "description": "Enclosure Pro Project File",
        "role": "Editor",
        "icon": "EnclosureProFileIcon.ico"
      }
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "universal"
          ]
        }
      ],
      "artifactName": "Enclosure-Pro-${version}.${ext}",
      "icon": "EnclosureProIcon.icns",
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeExtensions": [
              "enc"
            ],
            "CFBundleTypeName": "Enclosure Pro Project",
            "CFBundleTypeRole": "Editor",
            "CFBundleTypeIconFile": "EnclosureProFileIcon.icns"
          }
        ]
      }
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "artifactName": "Enclosure-Pro-${version}.${ext}",
      "category": "Graphics",
      "maintainer": "Tim Samoff <samoff@gmail.com>",
      "synopsis": "Professional effects pedal enclosure designer",
      "description": "Design drill hole layouts for effects pedal enclosures",
      "mimeTypes": [
        "application/x-enclosurepro"
      ],
      "fileAssociations": [
        {
          "ext": "enc",
          "name": "Enclosure Pro Project",
          "mimeType": "application/x-enclosurepro",
          "icon": "EnclosureProFileIcon.png"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Enclosure Pro",
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "license": "LICENSE",
      "deleteAppDataOnUninstall": false,
      "perMachine": true,
      "artifactName": "Enclosure-Pro-Setup-${version}.${ext}"
    },
    "portable": {
      "artifactName": "Enclosure-Pro-${version}.${ext}"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-menubar": "^1.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.6",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.60.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.2.1",
    "date-fns": "^3.6.0",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.13.1",
    "html2canvas": "^1.4.1",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.453.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.4.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5",
    "vaul": "^1.1.2",
    "wait-on": "^9.0.3",
    "wouter": "^3.3.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.3",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.4.47",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.6.3",
    "vite": "^7.2.4"
  }
}
